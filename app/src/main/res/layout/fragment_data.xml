            app:layout_constraintTop_toBottomOf="@+id/txtLowValue" />

        <TextView
            android:id="@+id/txtCloseValue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
